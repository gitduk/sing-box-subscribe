{
  "log": {
    "level": "info",
    "timestamp": true
  },
  "experimental": {
    "cache_file": {
      "enabled": true,
      "store_fakeip": true,
      "path": ".cache.db",
      "cache_id": "cache_id"
    },
    "clash_api": {
      "external_controller": "127.0.0.1:9090",
      "external_ui": "ui",
      "external_ui_download_url": "https://github.com/MetaCubeX/Yacd-meta/archive/gh-pages.zip",
      "external_ui_download_detour": "🚀 节点选择",
      "default_mode": "rule"
    }
  },
  "ntp": {
    "enabled": false,
    "server": "ntp.tencent.com",
    "server_port": 123,
    "interval": "30m",
    "detour": "direct"
  },
  "dns": {
    "servers": [
      {
        "tag": "dns_resolver",
        "type": "udp",
        "server": "223.5.5.5"
      },
      {
        "tag": "dns_direct",
        "type": "tls",
        "server": "dns.alidns.com",
        "domain_resolver": "dns_resolver"
      },
      {
        "tag": "dns_google",
        "type": "tls",
        "server": "dns.google",
        "domain_resolver": "dns_resolver",
        "detour": "🚀 节点选择"
      },
      {
        "tag": "dns_cflare",
        "type": "tls",
        "server": "one.one.one.one",
        "domain_resolver": "dns_resolver",
        "detour": "🚀 节点选择"
      },
      {
        "tag": "fakeip",
        "type": "fakeip",
        "inet4_range": "198.18.0.0/15",
        "inet6_range": "fc00::/18"
      }
    ],
    "rules": [
      {
        "clash_mode": "direct",
        "server": "dns_direct"
      },
      {
        "clash_mode": "global",
        "server": "dns_cflare"
      },
      {
        "rule_set": [
          "inline-cn",
          "inline-direct"
        ],
        "server": "dns_direct"
      },
      {
        "rule_set": [
          "geosite-anti-ad",
          "geosite-adguard",
          "geosite-category-ads-all"
        ],
        "action": "predefined",
        "rcode": "REFUSED"
      },
      {
        "rule_set": [
          "geoip-cn",
          "geosite-cn",
          "geosite-geolocation-cn"
        ],
        "server": "dns_direct"
      }
    ],
    "final": "dns_google",
    "strategy": "ipv4_only",
    "disable_expire": false,
    "independent_cache": true
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "address": [
        "172.18.0.1/30",
        "fdfe:dcba:9876::1/126"
      ],
      "auto_route": true,
      "auto_redirect": true,
      "strict_route": false,
      "route_address": [
        "0.0.0.0/1",
        "128.0.0.0/1",
        "::/1",
        "8000::/1"
      ],
      "route_exclude_address": [
        "192.168.0.0/16",
        "fc00::/7"
      ],
      "exclude_package": [
        "com.tencent.mm",
        "com.tencent.mp",
        "com.tencent.mobileqq",
        "com.tencent.qqmusic",
        "com.tencent.wemeet.app",
        "com.alicloud.databox",
        "com.alibaba.aliyun",
        "com.alibaba.android.rimet",
        "com.taobao.taobao",
        "com.taobao.idlefish",
        "com.bilibili.app.in",
        "com.eg.android.AlipayGphone",
        "com.unionpay",
        "com.sankuai.meituan",
        "com.autonavi.minimap",
        "com.baidu.BaiduMap",
        "com.netease.cloudmusic",
        "com.coolapk.market",
        "com.jingdong.app.mall",
        "nutstore.android"
      ]
    },
    {
      "type": "mixed",
      "tag": "mixed-in",
      "listen": "::",
      "listen_port": 7890,
      "set_system_proxy": false
    },
    {
      "type": "tproxy",
      "tag": "tproxy-in",
      "listen": "::",
      "listen_port": 7891
    },
    {
      "type": "redirect",
      "tag": "redirect-in",
      "listen": "::",
      "listen_port": 7897
    },
    {
      "type": "socks",
      "tag": "socks-in",
      "listen": "::",
      "listen_port": 7895
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "tag": "🚀 节点选择",
      "type": "selector",
      "outbounds": [
        "🇭🇰 香港节点",
        "🇨🇳 台湾节点",
        "🇸🇬 狮城节点",
        "🇯🇵 日本节点",
        "🇺🇲 美国节点",
        "🇰🇷 韩国节点",
        "GLOBAL"
      ]
    },
    {
      "tag": "🅖  Google",
      "type": "selector",
      "outbounds": [
        "🚀 节点选择",
        "🇯🇵 日本节点",
        "🇺🇲 美国节点",
        "🇰🇷 韩国节点",
        "GLOBAL"
      ]
    },
    {
      "tag": "💬 OpenAi",
      "type": "selector",
      "outbounds": [
        "🇯🇵 日本节点",
        "🇺🇲 美国节点",
        "🇰🇷 韩国节点",
        "🚀 节点选择",
        "GLOBAL"
      ]
    },
    {
      "tag": "✈️ Foreign",
      "type": "selector",
      "outbounds": [
        "🇯🇵 日本节点",
        "🇺🇲 美国节点",
        "🇰🇷 韩国节点",
        "🚀 节点选择",
        "GLOBAL"
      ]
    },
    {
      "tag": "🇭🇰 香港节点",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇭🇰|HK|hk|香港|港|HongKong"
          ]
        }
      ]
    },
    {
      "tag": "🇨🇳 台湾节点",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇹🇼|TW|tw|台湾|臺灣|台|Taiwan|TPE"
          ]
        }
      ]
    },
    {
      "tag": "🇸🇬 狮城节点",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇸🇬|SG|sg|新加坡|狮|Singapore|SIN"
          ]
        }
      ]
    },
    {
      "tag": "🇯🇵 日本节点",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇯🇵|JP|jp|日本|日|Japan|KIX|NRT"
          ]
        }
      ]
    },
    {
      "tag": "🇺🇲 美国节点",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇺🇸|US|us|美国|美|United States|SJC|LAX"
          ]
        },
        {
          "action": "exclude",
          "keywords": [
            "频道"
          ]
        }
      ]
    },
    {
      "tag": "🇰🇷 韩国节点",
      "type": "urltest",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "🇰🇷|韩国|KR|ICN"
          ]
        }
      ]
    },
    {
      "tag": "🎯 全球直连",
      "type": "selector",
      "outbounds": [
        "direct",
        "🚀 节点选择"
      ]
    },
    {
      "tag": "🌈 META",
      "type": "selector",
      "outbounds": [
        "{all}"
      ],
      "filter": [
        {
          "action": "include",
          "keywords": [
            "解锁|失联|订阅|频道"
          ]
        }
      ]
    },
    {
      "tag": "GLOBAL",
      "type": "selector",
      "outbounds": [
        "{all}",
        "direct"
      ],
      "filter": [
        {
          "action": "exclude",
          "keywords": [
            "解锁|失联|订阅|频道"
          ]
        }
      ]
    }
  ],
  "route": {
    "default_domain_resolver": {
      "server": "dns_resolver",
      "rewrite_ttl": 60,
      "client_subnet": "1.0.1.0"
    },
    "rules": [
      {
        "action": "sniff",
        "timeout": "1s"
      },
      {
        "type": "logical",
        "mode": "or",
        "rules": [
          {
            "port": 53
          },
          {
            "protocol": "dns"
          }
        ],
        "action": "hijack-dns"
      },
      {
        "clash_mode": "direct",
        "outbound": "direct"
      },
      {
        "clash_mode": "global",
        "outbound": "GLOBAL"
      },
      {
        "rule_set": [
          "inline-google"
        ],
        "outbound": "🅖  Google"
      },
      {
        "rule_set": [
          "inline-openai"
        ],
        "outbound": "💬 OpenAi"
      },
      {
        "rule_set": [
